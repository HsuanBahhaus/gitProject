<!-- inline scripts related to this page -->
<script type="text/javascript">
	jQuery(function($) {
		// jquery彈出視窗
		//override dialog's title function to allow for HTML titles
		$.widget("ui.dialog", $.extend({}, $.ui.dialog.prototype, {
			_title: function (title) {
				var $title = this.options.title || '&nbsp;'
				if (("title_html" in this.options) && this.options.title_html == true)
					title.html($title);
				else title.text($title);
			}
		}));

		// 彈出night-05-06-btn-modal-01
		$("#night-05-06-btn-modal-01").on('click', function (e) {
			e.preventDefault();
			var dialog = $("#night-05-06-btn-modal-01-message").removeClass('hide').dialog({
				modal: true,
				title: "<div class='widget-header widget-header-small'><h4 class='smaller'>開關班</h4></div>",
				title_html: true,
				width: 1000,
				maxWidth: 1920
			});
		});
		// 彈出night-05-06-btn-modal-02
		$("#night-05-06-btn-modal-02").on('click', function (e) {
			e.preventDefault();
			var dialog = $("#night-05-06-btn-modal-02-message").removeClass('hide').dialog({
				modal: true,
				title: "<div class='widget-header widget-header-small'><h4 class='smaller'>夜核日誌</h4></div>",
				title_html: true,
				width: 1000,
				maxWidth: 1920
			});
		});
	});
</script>


